generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("PRISMA_SQL_DATABASE_SERVICE_CONNECTION_STRING")
}

model account {
  id    Int     @id @default(autoincrement())
  name  String?
  email String?
}

model configuration {
  id                    Int     @id @default(autoincrement())
  readme                String?
  openapi               Json
  logo_only             String
  logo_label_light_mode String
  logo_label_dark_mode  String
}

model page {
  id            Int     @id @default(autoincrement())
  logo          String?
  slug          String
  title         String
  description   String
  type          String
  position      String
  parent        String
  content       Json
  configuration Json
  children      Json
}
